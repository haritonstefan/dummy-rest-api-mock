{
  "name": "dummy-rest-api-mock",
  "version": "0.0.1",
  "description": "Dummy rest api to use in development, or for demo",
  "scripts": {
    "start": "node dist/server.js",
    "transpile": "BABEL_ENV=production node_modules/.bin/babel -x \".es6\" app/ --out-dir dist/ --presets es2015",
    "watchAndTranspile": "BABEL_ENV=production node_modules/.bin/babel -x \".es6\" app/ --out-dir dist/ --presets es2015 --watch",
    "postinstall": "npm run transpile"
  },
  "keywords": [
    "API",
    "REST",
    "Flexible",
    "Dummy",
    "Easy"
  ],
  "author": "Hariton Stefan <hariton.stefan@outlook.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0"
  },
  "dependencies": {
    "blipp": "^2.3.0",
    "boom": "^4.2.0",
    "good": "^7.1.0",
    "good-console": "^6.2.0",
    "hapi": "^16.0.2",
    "hoek": "^4.1.0",
    "joi": "^10.1.0",
    "lokijs": "^1.4.2"
  }
}
